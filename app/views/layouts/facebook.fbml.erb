<fb:fbml>
	<% if false -%>
		<% fb_dashboard do -%>
			<%= fb_action "Invite your friends", {:controller => 'registration', :action => 'setup_friends'} %>
			<%= fb_help "About", fb_about_url %>
			<%= fb_help "Help & Feedback", {:controller => 'pages', :action => 'contact'} %>
		<% end -%>
	<% end -%>
	<%= fb_dashboard %>
	<% fb_tabs do -%>
		<%= fb_tab_item "Friends",	'#', :selected => false %>
	  <%= fb_tab_item "Subscriptions",			'#', :selected => false %>
		<%= fb_tab_item "Videos",							'#', :selected => false %>
	<% end -%>
	<%= yield  %>
</fb:fbml>