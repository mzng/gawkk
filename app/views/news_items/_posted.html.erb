<% if !video.posted_by.feed_owner? -%>
	<table class="inline" cellpadding="0" cellspacing="0">
		<tr>
			<td class="gutter">&nbsp;</td>
			<td class="image-small">
				<%= image_tag('add.png') %>
			</td>
			<td colspan="2">
				Originally posted by <%= link_to video.posted_by.username, user_path(:id => video.posted_by) %>
			</td>
		</tr>
	</table>
<% end -%>