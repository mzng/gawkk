<% if tagged -%>
	<table class="inline" cellpadding="0" cellspacing="0">
		<tr>
			<td class="image">
				<%= image_tag('tag_green.png') %>
			</td>
			<td colspan="2">
				<%= render :partial => "/tags/tag", :collection => video.tags, :spacer_template => '/tags/tag_spacer' %>
			</td>
		</tr>
	</table>
<% end -%>