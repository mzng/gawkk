<%= render :partial => "/layouts/content_header_simple", :locals => {:title => 'Members', :crumbs => [{:name => 'Members', :params => {:controller => 'users'}}]} %>

<div id="content-main">
	<h1 class="section-header">Search Members</h1>
	<%= render :partial => "/search/search", :locals => {:link => {:controller => 'search', :action => 'members'}, :separated => false} %>
	
	<h1 class="section-header">Browse Members</h1>
	<div class="grid-container">
		<%= render @users %>
	</div>
	<%= render :partial => "/layouts/pagination", :locals => {:collection => @users} %>
</div>

<div id="content-sidebar">
	
</div>